{
  "name": "chatbot-development",
  "version": "1.0.0",
  "description": "Intelligent chatbot with multi-API support and comprehensive development framework",
  "main": "src/index.js",
  "scripts": {
    "dev": "npx killport 3333 && next dev -p 3333",
    "dev:clean": "npx killport 3333 && next dev -p 3333",
    "dev:debug": "npx killport 3333 && NODE_OPTIONS='--inspect' next dev -p 3333",
    "start": "next start -p 3333",
    "build": "next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "killport": "npx killport 3333",
    "setup": "node scripts/pre-dev-checks.js && npx killport 3333",
    "docs": "node scripts/generate-run-report.js",
    "deploy:netlify": "node scripts/deploy-netlify.js",
    "build:netlify": "npm run build",
    "preview:netlify": "npx netlify dev"
  },
  "keywords": [
    "chatbot",
    "ai",
    "nextjs",
    "typescript",
    "nodejs",
    "react"
  ],
  "author": "Chatbot Development Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.75.1",
    "dotenv": "^17.2.3",
    "next": "^15.5.6",
    "openai": "^6.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "killport": "^1.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/chatbot-development.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/chatbot-development/issues"
  },
  "homepage": "https://github.com/your-org/chatbot-development#readme"
}
